<!-- game_started -->

<div class="content">
   <div class="play">
      {{#each buttons}}
         <button id="{{id}}" class="play">{{text}}</button>
      {{/each}}
   </div>
   <div class="controls">
      <h2><strong>Score: </strong>0</h2>
      <button class="good"><img src="images/pause.svg" alt="pause/edit game" /></button>
      <button class="bad"><img src="images/stop.svg" alt="end game" /></button>
   </div>
</div>
